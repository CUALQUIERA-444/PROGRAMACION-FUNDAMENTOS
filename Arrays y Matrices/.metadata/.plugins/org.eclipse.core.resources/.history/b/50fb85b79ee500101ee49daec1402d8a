package ejerciciosarrays;

import java.util.Scanner;

public class Ejercicio8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//8. Dado un array de 10 números decid cual es el número que más se repite.
		//int numeros[] = new int [10];
		int numeros[] = {0,6,6,6,6,1,2,3,3,2};
		//int numeros2[] = {0,6,6,6,6,1,2,3,3,2};
		int contador[] = new int[10];
		int cuenta_numeros = 0;
		Scanner sc = new Scanner(System.in);
		//Mostrar array
		for (int i = 0; i < numeros.length; i++) {
			System.out.println("Mostrar array "+numeros[i]);
		}
		//Recorrer array y crear el array de contador 
		for (int j = 0; j < numeros.length; j++) {
			for (int k = 0; k < contador.length;k++) {
		     if(numeros[j] == numeros[k])
		    	 contador[k] = cuenta_numeros++;
		     System.out.println(contador[k]);
		}
	}
	}
}
